{
  "hosting": {
    "public": "dist",
    "rewrites": [
      {
        "source": "!/filepage/**",
        "destination": "/index.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "cleanUrls": true,
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"]
  }
}
